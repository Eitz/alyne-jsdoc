{
  "name": "alyne-jsdoc",
  "version": "0.1.0",
  "description": "A clean, responsive documentation template theme with the Alyne colors for JSDoc 3 inspired by alyne-jsdoc",
  "main": "publish.js",
  "scripts": {
    "test": "jsdoc -c fixtures/fixtures.conf.json",
    "sync": "browser-sync start -s ../fixtures-doc -f ../fixtures-doc --reload-delay 1000 --no-ui --no-notify",
    "watch": "watch-run -d 1000 -p tmpl/**,static/** \"npm run test\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/manuel-reil/alyne-jsdoc.git"
  },
  "devDependencies": {
    "jsdoc": "latest",
    "browser-sync": "latest",
    "watch-run": "latest"
  },
  "author": "Manuel Reil <manuel.reil@alyne.com>",
  "license": "Apache-2.0",
  "keywords": [
    "jsdoc",
    "template",
    "alyne"
  ]
}
